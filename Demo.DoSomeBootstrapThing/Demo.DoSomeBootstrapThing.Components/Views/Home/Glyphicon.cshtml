@model System.Collections.Generic.List<string>

<script src="//cdnjs.cloudflare.com/ajax/libs/jquery.isotope/2.2.0/isotope.pkgd.min.js"></script>

@{
	ViewBag.Title = "Glyphicon";
}

<style>
	.grid-item {
		float: left;
		width: 200px;
		height: 50px;
		background: #e6e5e4;
		border: 2px solid #b6b5b4;
	}

	.grid-item--width2 {
		width: 50%;
	}
</style>

<h2>Glyphicon</h2>

<script language="javascript">
	$(function() {
		$('.grid').isotope({
			sortBy: "name",
			itemSelector: '.grid-item',
			layoutMode: 'fitRows',

			getSortData: {
				name: '.name'
			}
		});
	});
</script>

<div class="row">
	<div class="grid">
		@foreach (string glyphicon in @Model)
		{
			<div class="element-item">
				<div class="grid-item glyphicon @glyphicon">
					<h5 class="name">@glyphicon</h5>
				</div>
			</div>
		}
	</div>
</div>

















